{
  "name": "jdlabs/spaniel",
  "description": "A Wordpress Plugin Helper",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "authors": [
    {
      "name": "Javier De la Hoz",
      "email": "javierdlahoz@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "composer/installers": "v1.7.0",
    "guzzlehttp/guzzle": "^7.2",
    "zeptech/annotations": "^1.1",
    "haydenpierce/class-finder": "^0.4.3"
  },
  "autoload": {
    "psr-4": {
      "Jdlabs\\Spaniel\\": "src/"
    },
    "files": [
      "include/functions_include.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}